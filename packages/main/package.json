{
  "name": "@serlo/athene2-assets",
  "version": "4.0.0",
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:assets": "rimraf dist",
    "build:assets": "cross-env NODE_ENV=production webpack --config webpack.prod.config",
    "deploy": "ts-node scripts/deploy",
    "postdeploy": "yarn sentry-release",
    "sentry-release": "run-s sentry-release:create sentry-release:set-commits sentry-release:deploy",
    "sentry-release:create": "sentry-cli releases new --project athene2-assets athene2-assets@$npm_package_version",
    "sentry-release:set-commits": "sentry-cli releases set-commits --auto athene2-assets@$npm_package_version",
    "sentry-release:deploy": "sentry-cli releases deploys athene2-assets@$npm_package_version new --env production",
    "start": "webpack-dev-server --config webpack.dev.config"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@sentry/browser": "^4.0.0",
    "@serlo/editor": "^0.5.0",
    "@serlo/editor-helpers": "^4.0.0",
    "@serlo/legacy-editor-to-editor": "^4.0.0",
    "@serlo/markdown": "^4.0.0",
    "algebra.js": "^0.2.0",
    "autosize": "^4.0.0",
    "blueimp-file-upload": "^9.0.0",
    "blueimp-load-image": "^2.0.0",
    "body-parser": "^1.0.0",
    "bootstrap-datepicker": "^1.0.0",
    "bootstrap-sass": "^3.0.0",
    "d3": "^3.0.0",
    "dnode": "^1.0.0",
    "font-awesome": "^4.0.0",
    "howler": "^2.0.0",
    "inquirer": "^6.0.0",
    "jasny-bootstrap": "^3.0.0",
    "jquery": "^3.0.0",
    "jquery-sticky": "^1.0.0",
    "jquery-ui": "^1.0.0",
    "jquery.redirect": "^1.0.0",
    "lodash": "^4.0.0",
    "lodash.debounce": "^4.0.0",
    "magnific-popup": "^1.0.0",
    "moment": "^2.0.0",
    "ramda": "^0.26.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "selection-position": "^1.0.0",
    "string": "^3.0.0",
    "styled-components": "^4.0.0",
    "underscore": "^1.0.0",
    "unexpected": "^10.0.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@serlo/cloudflare": "^4.0.0",
    "@serlo/gcloud": "^4.0.0",
    "@types/inquirer": "^0.0.43",
    "@types/jquery": "^3.0.0",
    "@types/lodash": "^4.0.0",
    "@types/ramda": "^0.26.0",
    "@types/react-dom": "^16.0.0",
    "@types/request": "^2.0.0",
    "@types/semver": "^5.0.0",
    "@types/styled-components": "^4.0.0",
    "babel-loader": "^8.0.0",
    "cross-env": "^5.0.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.0",
    "ignore-loader": "^0.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.0.0",
    "nodemon": "^1.0.0",
    "npm-run-all": "^4.0.0",
