    "prebuild:assets": "rimraf dist",
    "build:assets": "webpack --config webpack.main.prod.config",
    "prebuild:cf": "rimraf dist-cf",
    "build:cf": "webpack --config-register babel-register --config webpack.cf.config",
    "prebuild:gcf": "rimraf dist-gcf",
    "build:gcf": "webpack --config webpack.gcf.config",
    "deploy": "ts-node scripts/deploy",
    "deploy:cf": "ts-node scripts/deploy-cf",
    "format": "prettier --write \"{{__tests__,src/**/*,*},*}.{js,jsx,ts,tsx,css,scss,json,md,markdown}\"",
    "license": "ts-node scripts/license-headers",
    "start": "webpack-dev-server --config webpack.main.dev.config",
    "test": "jest",
    "server": "nodemon -w src/ory-editor --exec \"node src/editor/index.server.js\""
