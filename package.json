  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "webpack --config webpack.config.prod -p",
    "build:cf": "webpack --config webpack.config.cf",
    "commit": "git-cz",
    "format":
      "prettier-standard \"{{__tests__,src}/**/*,*}.{js,jsx,ts,tsx,css,less,scss,json}\"",
    "postformat": "prettier --write \"{{__tests__,src}/**/*,*}.md\"",
    "lint": "standard \"{{__tests__,src}/**/*,*}.js\" | snazzy",
    "semantic-release":
      "semantic-release pre && npm publish && semantic-release post",
    "start": "webpack-dev-server --config webpack.config.dev",
    "test": "jest"
  },
  "files": ["dist"],
  "dependencies": {
    "algebra.js": "^0.2.6",
    "autosize": "^4.0.0",
    "blueimp-file-upload": "^9.19.1",
    "blueimp-load-image": "^2.17.0",
    "bootstrap-datepicker": "^1.7.1",
    "bootstrap-sass": "^3.3.7",
    "d3": "^3.5.17",
    "font-awesome": "^4.7.0",
    "howler": "^2.0.5",
    "jasny-bootstrap": "^3.1.3",
    "jquery": "^3.2.1",
    "jquery-sticky": "^1.0.4",
    "jquery-ui": "^1.12.1",
    "magnific-popup": "^1.1.0",
    "moment": "^2.19.1",
    "ramda": "^0.25.0",
    "showdown": "^1.8.1",
    "string": "^3.3.3",
    "underscore": "^1.8.3"
