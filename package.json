    "commitizen": "^2.9.6",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "jest": "^21.2.1",
    "node-sass": "^4.6.0",
    "prettier": "^1.8.2",
    "prettier-standard": "^7.0.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "semantic-release": "^8.2.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
